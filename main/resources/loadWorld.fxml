<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.stage.Stage?>

<Stage title="loadWorld" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.WorldController">
    <scene>
        <Scene>
            <AnchorPane fx:id="anchorPane" prefHeight="321.0" prefWidth="416.0">
            <children>
               <Button fx:id="worldSelector" layoutX="295.0" layoutY="89.0" mnemonicParsing="false" onAction="#handleButtonAction1" prefHeight="23.0" prefWidth="103.0" text="Select File">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </Button>
               <Text layoutX="123.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="World Configuration" textAlignment="CENTER">
                  <font>
                     <Font name="System" size="18.0" />
                  </font>
               </Text>
               <RadioButton fx:id="importWorld" layoutX="30.0" layoutY="62.0" mnemonicParsing="false" text="Import Existing World">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </RadioButton>
               <RadioButton fx:id="customWorld" layoutX="30.0" layoutY="142.0" mnemonicParsing="false" text="Custom World">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </RadioButton>
               <Button fx:id="finishWorld" layoutX="326.0" layoutY="285.0" mnemonicParsing="false" onAction="#handleButtonAction2" prefHeight="23.0" prefWidth="75.0" text="Finish">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </Button>
               <Button fx:id="closeWorld" layoutX="245.0" layoutY="285.0" mnemonicParsing="false" onAction="#handleButtonAction3" prefHeight="23.0" prefWidth="75.0" text="Close">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </Button>
               <TextField fx:id="worldHeight" layoutX="102.0" layoutY="164.0" prefHeight="9.0" prefWidth="40.0">
                  <font>
                     <Font size="11.0" />
                  </font></TextField>
               <Text layoutX="52.0" layoutY="178.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Height:">
                  <font>
                     <Font name="System" size="12.0" />
                  </font></Text>
               <Text layoutX="52.0" layoutY="208.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Width:">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </Text>
               <TextField fx:id="worldWidth" layoutX="102.0" layoutY="194.0" prefHeight="9.0" prefWidth="40.0">
                  <font>
                     <Font size="11.0" />
                  </font>
               </TextField>
               <ListView fx:id="listview" layoutX="52.0" layoutY="88.0" prefHeight="27.0" prefWidth="232.0" />
               <RadioButton fx:id="randomWorld" layoutX="30.0" layoutY="240.0" mnemonicParsing="false" text="Random World">
                  <font>
                     <Font name="System" size="12.0" />
                  </font>
               </RadioButton>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
